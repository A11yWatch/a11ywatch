  "author": "Jeffrey Mendez",
  "license": "MIT",
  "devDependencies": {
    "jest": "^26.4.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.0",
    "ts-node": "^8.9.1",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "0.0.9",
    "typescript": "^3.8.3"
